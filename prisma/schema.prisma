generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Match {
  id        String    @id @default(uuid())
  map       String
  rounds    Round[]
  winner    String
}

model Round {
  id        String    @id @default(uuid())
  number    Int
  winner    String
  matchId   String
  match     Match     @relation(fields: [matchId], references: [id])
  winMethod String
}
